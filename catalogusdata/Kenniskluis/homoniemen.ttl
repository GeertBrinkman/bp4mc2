@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos:	<http://www.w3.org/2004/02/skos/core#>.
@prefix bag:	<http://bag.basisregistraties.overheid.nl/id/begrip/> .
@prefix kdstat:	<http://kadaster.basisregistraties.overheid.nl/id/status/> .
@prefix kddef:	<http://kadaster.basisregistraties.overheid.nl/som/def#>.
@prefix brk:	<http://brk.basisregistraties.overheid.nl/id/begrip/>.

brk:hond rdf:type skos:Concept ;
	rdfs:label "hond"@nl ;
	rdfs:label "dog"@en ;
	kddef:status kdstat:Gevalideerd ;
	skos:prefLabel "pref hond"@nl ;
	rdfs:comment "Dit is een uitleg voor het concept hond" ;
	skos:altLabel "puppy";
.


bag:hond rdf:type skos:Concept ;
	rdfs:label "hond"@nl ;
	rdfs:label "dog"@en ;
	kddef:status kdstat:Gevalideerd ;
	skos:prefLabel "pref hond"@nl ;
	rdfs:comment "Dit is een uitleg voor het concept hond" ;
.
