#################################################################
#    tbv Waardelijst SoortGrootte
#################################################################

brk_col:SoortGrootte rdf:type skos:Collection ;
  rdfs:label "soort grootte" ;
  skos:prefLabel "soort grootte" ;
  skos:member
    brk_beg:Vastgesteld_soortGrootte,
    brk_beg:NietVastgesteld_soortGrootte,
    brk_beg:Geschat_soortGrootte,
    brk_beg:NietGeschat_soortGrootte
.

brk_beg:Vastgesteld_soortGrootte rdf:type skos:Concept ;
  rdfs:label "Vastgesteld (soort grootte)" ;
