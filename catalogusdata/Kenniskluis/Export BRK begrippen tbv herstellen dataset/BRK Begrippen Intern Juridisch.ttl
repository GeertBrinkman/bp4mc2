n1:Bestuur rdf:type skos:Concept;
    rdfs:label "Bestuur"^^xsd:string;
    rdfs:comment "Het bestuur is een onderdeel van een rechtspersoon die beslissingen neemt, volgens de regels die het bestuur zelf heeft gemaakt."^^xsd:string;
    skos:editorialNote "Ontleend  aan het burgerlijk wetboek, artikel 2:291 lid 1"^^xsd:string;
    skos:editorialNote "Ontleend aan het burgerlijk wetboek, artikel 2:239 lid 1"^^xsd:string;
    skos:editorialNote "Ontleend  aan het burgerlijk wetboek, artikel 2:129 lid 2"^^xsd:string;
    skos:editorialNote "Ontleend aan het burgerlijk wetboek, artikel 2:37"^^xsd:string;
