    skos:prefLabel "State"@en;
    skos:prefLabel "Rijk"@nl;
    skos:scopeNote "Het Rijk betreft de centrale overheid. Hieronder staan de lagere overheden: provincie, gemeenten en waterschap"^^xsd:string;
