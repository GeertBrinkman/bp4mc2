    skos:prefLabel "Reservefonds"^^xsd:string;
    skos:relatedMatch n1:Appartementsrecht;
    skos:semanticRelation n1:Vereniging_van_eigenaren;
    ns5:signature "34b6c8ee71e1af860a5789c0292a06ae"
.
n1:Rijk rdf:type skos:Concept;
    rdfs:label "Rijk"@nl;
    rdfs:label "State"@en;
    rdfs:comment "Het Rijk is de overheidsorganisatie die het Nederlands grondgebied bestuurt."^^xsd:string;
    ns3:broaderGeneric n1:Openbaar_lichaam;
    ns3:broaderGeneric n1:Publiekrechtelijke_rechtspersoon;
