@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos:	<http://www.w3.org/2004/02/skos/core#>.
@prefix kdstat:	<http://kadaster.basisregistraties.overheid.nl/id/status/> .
@prefix kddef:	<http://kadaster.basisregistraties.overheid.nl/som/def#>.
@prefix brk:	<http://brk.basisregistraties.overheid.nl/id/concept/>.

brk:hond rdf:type skos:Concept ;
	rdfs:label "Hond"@nl ;
	rdfs:label "Dog"@en ;
	kddef:status kdstat:Gevalideerd ;
	skos:prefLabel "hond" ;
	rdfs:comment "Dit is een uitleg voor het concept hond" ;
	skos:altLabel "puppy";
.

brk:pup rdf:type skos:Concept ;
	rdfs:label "pup"@nl ;
	kddef:status kdstat:Gevalideerd ;
	skos:prefLabel "pup" ;
	rdfs:comment "Dit is een uitleg voor het concept pup" ;
.

