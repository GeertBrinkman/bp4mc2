@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos:	<http://www.w3.org/2004/02/skos/core#>.
@prefix bag:	<http://bag.basisregistraties.overheid.nl/id/begrip/> .
@prefix kdstat:	<http://kadaster.basisregistraties.overheid.nl/id/status/> .
@prefix kddef:	<http://kadaster.basisregistraties.overheid.nl/som/def#>.

bag:hond rdf:type skos:Concept ;
	rdfs:label "Hond"@nl ;
	rdfs:label "Dog"@en ;
	kddef:status kdstat:Geverifieerd ;
	skos:prefLabel "hond" ;
	rdfs:comment "Dit is een uitleg voor het concept hond" ;
.

bag:puppy rdf:type skos:Concept ;
	rdfs:label "puppy"@nl ;
	kddef:status kdstat:Geverifieerd ;
	skos:prefLabel "puppy" ;
	rdfs:comment "Dit is een uitleg voor het concept puppy" ;
	skos:altLabel "pup";
.
