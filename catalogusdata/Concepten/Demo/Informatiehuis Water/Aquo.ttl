dso:Winmiddel rdf:type skos:Concept;
	rdfs:label "winmiddel";
	skos:prefLabel "Winmiddel";
	skos:definition '''een middel om grondwater, in afwijking van het natuurlijke stromingspatroon, in meer of mindere mate geforceerd te doen toestromen ''';
	skos:inScheme <http://data.acceptatie.pdok.nl/catalogus/dso/id/dataset/Water>;
