#---------------------
# Class
#---------------------

scdm:Class a sh:NodeShape;
  rdfs:label "Klasse"@nl;
  rdfs:label "Class"@en;
  sh:targetClass owl:Class;
  sh:name "owl:Class";
  sh:property scdm:Klasse_label;
  sh:property scdm:Klasse_onderwerp;
.
scdm:Klasse_label a sh:PropertyShape;
  rdfs:label "label"@nl;
  sh:name "rdfs:label";
  sh:path rdfs:label;
.
scdm:Klasse_onderwerp a sh:PropertyShape;
  rdfs:label "onderwerp"@nl;
  sh:name "dcterms:subject";
  sh:path dcterms:subject;
.

#---------------------
# Property
#---------------------
scdm:Eigenschap a sh:NodeShape;
  rdfs:label "Eigenschap"@nl;
  sh:targetClass rdf:Property;
  sh:name "rdf:Property";
  sh:property scdm:Eigenschap_onderwerp;
  sh:property scdm:Eigenschap_label;
.
scdm:Eigenschap_label a sh:PropertyShape;
  rdfs:label "label"@nl;
  sh:name "rdfs:label";
  sh:path rdfs:label;
.
scdm:Eigenschap_onderwerp a sh:PropertyShape;
  rdfs:label "onderwerp"@nl;
  sh:name "dcterms:subject";
  sh:path dcterms:subject;
.
