#---------------------
# IMPORT
#---------------------

#derived from adms
adms:Asset rdfs:subClassOf dcat:Dataset.
adms:AssetDistribution rdfs:subClassOf dcat:Distribution.

dmap:Asset a sh:NodeShape;
  sh:name "adms:Asset";
  sh:targetClass adms:Asset;
.
dmap:AssetDistribution a sh:NodeShape;
  sh:name "adms:AssetDistribution";
  sh:targetClass adms:AssetDistribution;
.

#derived from dqv
dqv:Dimension rdfs:subClassOf skos:Concept.
dqv:Category rdfs:subClassOf skos:Concept.

#---------------------
# Asset (DataModel)
#---------------------

dmap:DataModel rdf:type sh:NodeShape;
  rdfs:label "Asset (Informatiemodel)"@nl;
  rdfs:label "Asset (Data model)"@en;
  sh:name "adms:Asset (Data model)";
  rdfs:comment "Een informatiemodel is een beschrijving van de structuur waarin data over dingen in een  specifiek domein worden opgeslagen."@nl;
  rdfs:comment "An data model is a description of the structure in which data about things in a specific domain are stored."@en;
  skos:example "De lijst met Kadastrale gemeenten."@nl;
  sh:property dmap:DataModel_type;
  sh:property dmap:DataModel_typeDataModel;
  sh:property dmap:DataModel_shapesGraph;
.

dmap:DataModel_type a sh:PropertyShape;
  rdfs:label "getypeerd als asset"@nl;
  rdfs:label "typified as asset"@en;
  sh:name "rdf:type";
  sh:path rdf:type;
  sh:hasValue adms:Asset;
  rdfs:comment "Een informatiemodel wordt getypeerd als adms:Asset."@nl;
  rdfs:comment "A data model is typed as adms:Asset."@en;
  skos:example "IMKAD is vindbaar als een adms:Asset."@nl;
.
dmap:DataModel_typeDataModel a sh:PropertyShape;
  rdfs:label "type asset = informatiemodel"@nl;
   rdfs:label "type asset = data model"@en;
 sh:path dcterms:type;
  sh:name "dcterms:type";
  sh:minCount 1;
  sh:maxCount 1;
  sh:hasValue dmap:DataModel;
  rdfs:comment "Een informatiemodel wordt expliciet getypeerd als informatiemodel."@nl;
  rdfs:comment "A data model is explicitly typed as a data model."@en;
  skos:example ""@nl;
.

dmap:DataModel_shapesGraph rdf:type sh:PropertyShape;
  rdfs:label "is beschreven"@nl;
  rdfs:label "is described"@en;
  sh:name "sh:shapesGraph";
  sh:path sh:shapesGraph;
  sh:node dmap:ShapesGraph;
  rdfs:comment "Een informatiemodel kan worden beschreven in een 'shape graph'."@nl;
  rdfs:comment "A data model can be described in a shapes graph."@en;
  skos:example "De verwijzing naar de beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:message "Een informatiemodel kent maximaal 1 model beschrijving."@nl;
  sh:message "An data model has up to 1 shapes graph."@en
.

#---------------------
# ShapesGraph
#---------------------

dmap:ShapesGraph rdf:type sh:NodeShape;
  rdfs:label "model beschrijving"@nl;
  rdfs:label "Shapes graph"@nl;
  sh:name "Shapes graph";
  rdfs:comment "De beschrijving van een informatiemodel."@nl;
  rdfs:comment "A shapes graph of a data model."@en;
  skos:example "De beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
 .

 dmap:LandingPage a sh:NodeShape;
   rdfs:label "webpagina"@nl;
   rdfs:label "landing page"@en;
   sh:name "foaf:Document (Landing page)";
   sh:property [
     sh:path rdf:type;
     sh:hasValue foaf:Document;
   ];
   rdfs:comment "De webpagina va welke een asset vindbaar is."@nl;
   rdfs:comment "The landing page on the web on which an asset can be found."@en;
   skos:example "kadaster.nl/waardelijsten."@nl;
 .
