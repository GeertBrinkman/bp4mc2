@prefix scdm: <http://bp4mc2.org/somewhere/dm-ap-sc/>.
@prefix adms: <http://www.w3.org/ns/adms#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix prov: <http://www.w3.org/ns/prov#>.
@prefix dqv: <http://www.w3.org/ns/dqv#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dctype: <http://purl.org/dc/dcmitype/>.
@prefix wdrs: <http://www.w3.org/2007/05/powder-s#>.
@prefix skoslex: <http://bp4mc2.org/def/skos-lex#>.
@prefix skosthes: <http://purl.org/iso25964/skos-thes#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix dcmiperiod:   <http://dublincore.org/documents/2006/04/10/dcmi-period/>.
@prefix sd: <http://www.w3.org/ns/sparql-service-description#>.
@prefix frbr: <http://purl.org/vocab/frbr/core#>.
@prefix org: <http://www.w3.org/ns/org#>.
@prefix dstype: <http://bp4mc2.org/def/dataset-type#>.

#---------------------
# IMPORT
#---------------------

#derived from dataset-type
dstype:Datamodel a skos:Concept;
  rdfs:label "Informatiemodel"@nl;
  rdfs:label "Datamodel"@en;
  sh:name "dstype:Datamodel"
.
dstype:ControlledVocabulary a skos:Concept;
  rdfs:label "Waardelijst"@nl;
  rdfs:label "Controlled vocabulary"@en;
  sh:name "dstype:ControlledVocabulary"
.
dstype:DatasetVersion a skos:Concept;
  rdfs:label "Datasetversie"@nl;
  rdfs:label "Dataset version"@en;
  sh:name "dstype:DatasetVersion"
.

#derived from skos-lex
skoslex:Act rdfs:subClassOf skos:Concept.
skoslex:Object rdfs:subClassOf skos:Concept.
skoslex:Actor rdfs:subClassOf skos:Concept.
skoslex:Agent rdfs:subClassOf skoslex:Actor.

#derived from frbr
frbr:Work rdfs:subClassOf frbr:Endeavour.
frbr:Expression rdfs:subClassOf frbr:Endeavour.

#derived from prov
prov:Person rdfs:subClassOf prov:Agent.
prov:Organization rdfs:subClassOf prov:Agent.
prov:SoftwareAgent rdfs:subClassOf prov:Agent.

#derived from foaf
foaf:Organization rdfs:subClassOf foaf:Agent.

#derived from org
org:Organization rdfs:subClassOf foaf:Organization.

#derived from adms
adms:Asset rdfs:subClassOf dcat:Dataset.
adms:AssetDistribution rdfs:subClassOf dcat:Distribution.

#derived from dqv
dqv:Dimension rdfs:subClassOf skos:Concept.
dqv:Category rdfs:subClassOf skos:Concept.

#Extra
rdf:type sh:name "rdf:type".
dqv:value sh:name "dqv:value".
skos:inScheme sh:name "skos:inScheme".


#---------------------
# Asset (DataModel)
#---------------------

scdm:DataModel rdf:type sh:NodeShape;
  rdfs:label "Asset (Informatiemodel)"@nl;
  rdfs:label "Asset (Data model)"@en;
  sh:name "adms:Asset (Data model)";
  rdfs:comment "Een informatiemodel is een beschrijving van de structuur waarin data over dingen in een  specifiek domein worden opgeslagen."@nl;
  rdfs:comment "An data model is a description of the structure in which data about things in a specific domain are stored."@en;
  skos:example "De lijst met Kadastrale gemeenten."@nl;
  sh:property scdm:DataModel_type;
  sh:property scdm:DataModel_typeDataModel;
.

scdm:DataModel_type a sh:PropertyShape;
  rdfs:label "getypeerd als asset"@nl;
  rdfs:label "typified as asset"@en;
  sh:name "rdf:type";
  sh:path rdf:type;
  sh:hasValue adms:Asset;
  rdfs:comment "Een informatiemodel wordt getypeerd als adms:Asset."@nl;
  rdfs:comment "A data model is typed as adms:Asset."@en;
  skos:example "IMKAD is vindbaar als een adms:Asset."@nl;
.
scdm:DataModel_typeDataModel a sh:PropertyShape;
  rdfs:label "type asset = informatiemodel"@nl;
   rdfs:label "type asset = data model"@en;
 sh:path dcterms:type;
  sh:name "dcterms:type";
  sh:minCount 1;
  sh:maxCount 1;
  sh:hasValue dstype:Datamodel;
  rdfs:comment "Een informatiemodel wordt expliciet getypeerd als informatiemodel."@nl;
  rdfs:comment "A data model is explicitly typed as a data model."@en;
  skos:example ""@nl;
.

#---------------------
# Vocabulary
#---------------------

scdm:Vocabulary a sh:NodeShape;
  rdfs:label "Vocabulaire"@nl;
  rdfs:label "Vocabulary"@en;
  sh:name "owl:Ontology";
  sh:targetClass owl:Ontology;
  rdfs:comment "Een informatiemodel wordt beschreven in een vocabulaire dat alle klassen, eigenschappen, relaties, et cetera definieert, inclusief alle bijbehorende metadata."@nl;
  rdfs:comment "A data model is described in a vocabulary that defines all classes, properties, relations, et cetera, including all corresponding metadata."@en;
  skos:example "De beschrijving van de BRK in het InformatieModel Kadaster (IMKAD)."@nl;
  sh:property scdm:Vocabulary_shapesGraph;
  sh:property scdm:Vocabulary_isPrimaryTopicOf;
.

scdm:Vocabulary_shapesGraph rdf:type sh:PropertyShape;
  rdfs:label "is grafisch beschreven"@nl;
  rdfs:label "is graphically described"@en;
  sh:name "sh:shapesGraph";
  sh:path sh:shapesGraph;
  sh:node scdm:ShapesGraph;
  rdfs:comment "Een informatiemodel kan grafisch worden weergeven."@nl;
  rdfs:comment "A data model can be described in a shapes graph."@en;
  skos:example "De verwijzing naar de grafische beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:message "Een informatiemodel kent maximaal 1 grafische weergave."@nl;
  sh:message "An data model has up to 1 shapes graph."@en
.

scdm:Vocabulary_isPrimaryTopicOf rdf:type sh:PropertyShape;
  rdfs:label "is beschreven in"@nl;
  rdfs:label "is topic of"@en;
  sh:name "foaf:isPrimaryTopicOf (vocabulary)";
  sh:path foaf:isPrimaryTopicOf;
  sh:node scdm:Vocabulary;
  rdfs:comment "Een informatiemodel kan worden beschreven in een vocabulaire."@nl;
  rdfs:comment "A data model can be described in a vocabulary."@en;
  skos:example "De verwijzing naar de beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:message "Een informatiemodel kent maximaal 1 beschrijving."@nl;
  sh:message "An data model has up to 1 vocabulary."@en
.

#---------------------
# ShapesGraph
#---------------------

scdm:ShapesGraph rdf:type sh:NodeShape;
  rdfs:label "grafische weergave model"@nl;
  rdfs:label "Shapes graph"@nl;
  sh:name "Shapes graph";
  rdfs:comment "De grafische weergave van een vocabulaire."@nl;
  rdfs:comment "A shapes graph of a vocabulary."@en;
  skos:example "De grafische beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
  sh:property scdm:ShapesGraph_label;
.

scdm:ShapesGraph_label rdf:type sh:PropertyShape;
  rdfs:label "heeft label"@nl;
  rdfs:label "has label"@en;
  sh:name "rdfs:label (Shapes graph)";
  rdfs:comment "Een grafische weergave van een v heeft een ."@nl;
  rdfs:comment "A data model can be described in a vocabulary."@en;
  skos:example "De verwijzing naar de beschrijving van het InformatieModel Kadaster (IMKAD)."@nl;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:message "Een informatiemodel kent maximaal 1 beschrijving."@nl;
  sh:message "An data model has up to 1 vocabulary."@en
.



