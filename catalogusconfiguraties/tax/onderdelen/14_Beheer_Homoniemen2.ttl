#
#
#
# BEHEER HOMONIEMEN - 2
#
#
#


#Hoofdpagina Homoniemen - 2 Rapportage
stage:TaxHomoniemen2 a elmo:Representation;
	elmo:url-pattern "/query/homoniemen2$";
	elmo:contains stage:TaxHeader;
	elmo:contains stage:TaxFooter;
	elmo:contains stage:TaxBeheermenu;
	elmo:contains stage:TaxHomoniemen2Tabel;
	elmo:contains stage:TaxHomoniemen2Uitleg;
.

stage:TaxHomoniemen2Uitleg a elmo:Part;
	elmo:appearance elmo:HtmlAppearance;
	elmo:index "1";
	elmo:without-parameter "term";
	elmo:data [
		rdfs:label "Homoniemen2";
	]
.

# Weergave van homoniemen - 2
stage:TaxHomoniemen2Tabel a elmo:Part;
	elmo:appearance elmo:TableAppearance;
	elmo:index "2";
	elmo:query stage:TaxHomoniemen2Query;
	elmo:fragment [
		rdfs:label "Homoniem";						#de term die als homoniem voorkomt
		elmo:applies-to "homoniem";
	];
	elmo:fragment [
		rdfs:label "Populatie";						#het begrippenkader van het begrip
		elmo:applies-to "graph";
	];
	elmo:fragment [
		rdfs:label "Type";							#of het gaat om de begripsterm zelf, of een synoniem
		elmo:applies-to "pt1";
	];
	elmo:fragment [
		rdfs:label "Begrip";						#de URI van het begrip waar ook nog een homoniem van bestaat
		elmo:applies-to "begrip";
	];
	elmo:fragment [
		rdfs:label "Status";						#de status van dit begrip
		elmo:applies-to "status1";
	];
	elmo:fragment [
		rdfs:label "Komt ook voor in";				#het begrippenkader van het 'andere' begrip (homoniem)
		elmo:applies-to "graph2";
	];
	elmo:fragment [
		rdfs:label "Als";							#of het gaat om de term dan wel het synoniem van dat 'andere' begrip
		elmo:applies-to "pt2";
	];
	elmo:fragment [
		rdfs:label "Van begrip";					#de URI van het 'andere' begrip
		elmo:applies-to "begrip2";
	];
	elmo:fragment [
		rdfs:label "Met status";					#de status van het 'andere' begrip
		elmo:applies-to "status2";
	];
.

stage:TaxHomoniemen2Query a elmo:query;
	elmo:query '''
	'''
.