			BIND( IF( substr( "@PREFIX@", 2 ) = "PREFIX@",  "http://catalogus.test.kadaster.nl/def/", "@PREFIX@" ) AS ?prefix )
      BIND( IRI( CONCAT( ?prefix, REPLACE( "@NAME@", "[^A-Za-z0-9-_]", "" ) ) ) AS ?graph )
