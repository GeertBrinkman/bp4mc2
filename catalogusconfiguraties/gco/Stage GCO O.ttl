stage:dcat-nl_properties a elmo:Part;
	elmo:index "6";
	elmo:appearance elmo:ShortTableAppearance;
	elmo:fragment [
		elmo:applies-to "class";
		rdfs:label "Klasse";
	];
	elmo:fragment [
		elmo:applies-to "severity";
		rdfs:label "Req";
	];
	elmo:fragment [
		elmo:applies-to "property";
		rdfs:label "Property";
	];
	elmo:fragment [
		elmo:applies-to "uri";
		elmo:appearance elmo:GlobalLink;
		rdfs:label "URI";
	];
	elmo:fragment [
		elmo:applies-to "comment";
		rdfs:label "Toelichting";
	];
	elmo:fragment [
		elmo:applies-to "card";
		rdfs:label "Card";
	];
	elmo:fragment [
		elmo:applies-to "range";
		rdfs:label "Datatype";
	];
	elmo:query '''
		prefix sh: <http://www.w3.org/ns/shacl#>
		prefix dcat-nl: <http://bp4mc2.org/def/dcat-nl/>
		prefix dcterms: <http://purl.org/dc/terms/>
		select ?class ?class_label ?severity ?severity_label ?property ?uri ?uri_label ?comment ?card ?range ?range_label
		where {
			graph <http://bp4mc2.org/id/dataset/dcat-nl-profiel> {
				?shape sh:targetClass ?class.
				?shape sh:name ?class_label.
				?shape dcterms:type dcat-nl:Kernklasse.
				?shape sh:property ?prop.
				?prop sh:severity ?severity.
				?prop sh:path ?uri.
				?prop sh:name ?uri_label.
				?prop rdfs:label ?property.
				?prop rdfs:comment ?comment.
				OPTIONAL {?prop sh:minCount ?minc}
				OPTIONAL {?prop sh:maxCount ?maxc}
				BIND (concat(if(bound(?minc),?minc,"0"),"..",if(bound(?maxc),?maxc,"n")) as ?card)
				OPTIONAL {?prop sh:datatype ?datatype. ?datatype ^sh:targetClass/rdfs:label ?datatype_label FILTER (lang(?datatype_label)="nl")}
				OPTIONAL {?prop sh:class ?refclass. ?refclass ^sh:targetClass/rdfs:label ?refclass_label FILTER (lang(?refclass_label)="nl")}
				OPTIONAL {?prop sh:node ?refshape. ?refshape sh:name ?refshape_label}
				BIND (if(bound(?datatype),?datatype,if(bound(?refshape),?refshape,?refclass)) as ?range)
				BIND (if(bound(?datatype_label),?datatype_label,if(bound(?refshape_label),?refshape_label,?refclass_label)) as ?range_label)
			}
			graph <@STAGE@> {
				?severity rdfs:label ?severity_label.
				?severity rdf:value ?severity_level.
			}
		}
		order by ?class_label ?severity_level ?property
	''';
. 
@prefix elmo: <http://bp4mc2.org/elmo/def#>.
@prefix html: <http://www.w3.org/1999/xhtml/vocab#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix stage: <http://data.ontwikkeling.pdok.nl/catalogus/gco/stage#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

stage:dcat-vergelijking	a	elmo:Representation;
	elmo:url-pattern	"/query/dcat-vergelijking$";
	elmo:contains stage:Menu;
	elmo:contains stage:dcat-vergelijking_classes;
	elmo:contains stage:dcat-vergelijking_properties;
.

stage:dcat-vergelijking_classes a elmo:Part;
	elmo:index "1";
	elmo:appearance elmo:ShortTableAppearance;
	elmo:fragment [
		elmo:applies-to "class";
		elmo:appearance elmo:HiddenAppearance;
		elmo:name "SUBJECT";
	];
	elmo:fragment [
		elmo:applies-to "smodel";
		rdfs:label "DCAT (W3)";
	];
	elmo:fragment [
		elmo:applies-to "sapeu";
		rdfs:label "DCAT AP-EU";
	];
	elmo:fragment [
		elmo:applies-to "sapnl";
		rdfs:label "DCAT AP-NL";
	];
	elmo:fragment [
		elmo:applies-to "sgco";
		rdfs:label "DCAT GCO";
	];
	elmo:query '''
		prefix sh: <http://www.w3.org/ns/shacl#>
		select ?class ?smodel ?smodel_label ?sapeu ?sapeu_label ?sapnl ?sapnl_label ?sgco ?sgco_label
		where {
			{
				graph <http://bp4mc2.org/id/dataset/dcat-model> {
					?smodel rdf:type sh:NodeShape.
					?smodel sh:targetClass ?class.
					?smodel sh:name ?smodel_label
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/dcat-ap-eu> {
					?sapeu rdf:type sh:NodeShape.
					?sapeu sh:targetClass ?class.
					?sapeu sh:name ?sapeu_label
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/dcat-nl-profiel> {
					?sapnl rdf:type sh:NodeShape.
					?sapnl sh:targetClass ?class.
					?sapnl sh:name ?sapnl_label
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/bp4mc2-datasets> {
					?sgco rdf:type sh:NodeShape.
					?sgco sh:targetClass ?class.
					?sgco sh:name ?sgco_label
				}
			}
		}
	''';
.

stage:dcat-vergelijking_properties a elmo:Part;
	elmo:index "2";
	elmo:appearance elmo:ShortTableAppearance;
	elmo:fragment [
		elmo:applies-to "predicate";
		elmo:appearance elmo:HiddenAppearance;
		elmo:name "SUBJECT";
	];
	elmo:fragment [
		elmo:applies-to "pnmodel";
		rdfs:label "DCAT (W3)";
	];
	elmo:fragment [
		elmo:applies-to "psapeu";
		rdfs:label "DCAT AP-EU";
	];
	elmo:fragment [
		elmo:applies-to "psapnl";
		rdfs:label "DCAT AP-NL";
	];
	elmo:fragment [
		elmo:applies-to "pngco";
		rdfs:label "DCAT GCO";
	];
	elmo:query '''
		prefix sh: <http://www.w3.org/ns/shacl#>
		select	(iri(concat(str(?class),str(?property))) as ?predicate)
				?class ?class_label
				(if(bound(?pnmodel),?pnmodel,if(bound(?pnapeu),?pnapeu,if(bound(?pnapnl),?pnapnl,?pngco))) as ?property)
				?pnmodel
				?psapeu (concat(?ssapeulabel,"->",?psapeulabel) as ?psapeu_label)
				?psapnl (concat(?ssapnllabel,"->",?psapnllabel) as ?psapnl_label)
				?pngco
		where {
			{
				graph <http://bp4mc2.org/id/dataset/dcat-model> {
					?smodel rdf:type sh:NodeShape.
					?smodel sh:targetClass ?class.
					?smodel sh:name ?class_label.
					?smodel sh:property ?pmodel.
					?pmodel sh:path ?property.
					?pmodel sh:name ?pnmodel.
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/dcat-ap-eu> {
					?sapeu rdf:type sh:NodeShape.
					?sapeu sh:targetClass ?class.
					?sapeu sh:name ?class_label.
					?sapeu sh:property ?papeu.
					?sapeu sh:severity ?ssapeu.
					?papeu sh:path ?property.
					?papeu sh:name ?pnapeu.
					?papeu sh:severity ?psapeu.
				}
				graph <@STAGE@> {
					?psapeu rdfs:label ?psapeulabel.
					?ssapeu rdfs:label ?ssapeulabel
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/dcat-nl-profiel> {
					?sapnl rdf:type sh:NodeShape.
					?sapnl sh:targetClass ?class.
					?sapnl sh:name ?class_label.
					?sapnl sh:property ?papnl.
					?sapnl sh:severity ?ssapnl.
					?papnl sh:path ?property.
					?papnl sh:name ?pnapnl.
					?papnl sh:severity ?psapnl.
				}
				graph <@STAGE@> {
					?psapnl rdfs:label ?psapnllabel.
					?ssapnl rdfs:label ?ssapnllabel
				}
			}
			union
			{
				graph <http://bp4mc2.org/id/dataset/bp4mc2-datasets> {
					?sgco rdf:type sh:NodeShape.
					?sgco sh:targetClass ?class.
					?sgco sh:name ?class_label.
					?sgco sh:property ?pgco.
					?pgco sh:path ?property.
					?pgco sh:name ?pngco
				}
			}
		}
		order by ?class ?property
	''';
. 
@prefix elmo: <http://bp4mc2.org/elmo/def#>.
@prefix html: <http://www.w3.org/1999/xhtml/vocab#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix stage: <http://data.ontwikkeling.pdok.nl/catalogus/gco/stage#>.

stage:dcat-model	a	elmo:Representation;
	elmo:url-pattern	"/query/dcat(\\.(xml|ttl|json))?$";
	elmo:contains stage:Menu;
	elmo:appearance elmo:VocabularyAppearance;
	elmo:query '''
		prefix sh: <http://www.w3.org/ns/shacl#>
		construct {
			?s?p?o.
			?class rdf:type ?ctype.
			?class rdfs:comment ?cc.
			?prop rdf:type ?ptype.
			?prop rdfs:comment ?pc.
			?class rdfs:subClassOf ?parent
		}
		where {
			graph <http://bp4mc2.org/id/dataset/dcat-model> {
				?s?p?o
				OPTIONAL {
					?shape rdf:type sh:NodeShape.
					?shape sh:targetClass ?class.
					OPTIONAL {
						?shape sh:property ?ps.
						?ps sh:path ?prop.
					}
				}
			}
			graph <http://bp4mc2.org/id/dataset/imports> {
				?class rdf:type ?ctype.
				optional {
					?class rdfs:subClassOf ?parent
				}
				optional {
					?class rdfs:comment ?cc.
					filter (lang(?cc)="en")
				}
				optional {
					?prop rdf:type ?ptype.
					optional {
						?prop rdfs:comment ?pc.
						filter (lang(?pc)="en")
					}
				}
			}
		}
	''';
.
 
