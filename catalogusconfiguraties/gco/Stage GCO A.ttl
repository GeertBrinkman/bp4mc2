stage:sync_import_dcat a elmo:Scene;
	elmo:index "02";
	rdfs:label "dcat vocabulary import";
	elmo:query '''
		insert into <http://bp4mc2.org/id/dataset/bp4mc2> {
			?s?p?o
		}
