stage:FetchBp4mc2 a elmo:Query;
	rdfs:label "Bp4mc2 model vanaf github";
	elmo:query '''
		select *
		where {
			service <http://localhost:8080/catalogus/url> {
				graph <http://raw.githubusercontent.com/bp4mc2/bp4mc2/master/model/bp4mc2.ttl> {
					?s?p?o
				}
			}
		}
	''';
.
