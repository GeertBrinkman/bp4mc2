			elmo:data [ elmo:index "5"; rdfs:label "Document"; html:link "query/reqdocument"];
		];
		elmo:data [
			elmo:index "4";
			rdfs:label "DCAT";
			elmo:data [ elmo:index "1"; rdfs:label "Vocabulaire DCAT basis"; html:link "query/dcat"];
			elmo:data [ elmo:index "2"; rdfs:label "Vocabulaire AP-EU"; html:link "query/dcat-ap-eu"];
			elmo:data [ elmo:index "3"; rdfs:label "Lijst AP-EU"; html:link "query/dcat-ap-eu-lijst"];
			elmo:data [ elmo:index "4"; rdfs:label "Lijst AP-NL"; html:link "query/dcat-nl"];
			elmo:data [ elmo:index "5"; rdfs:label "Vocabulaire AP-NL"; html:link "query/dcat-nl-vocab"];
			elmo:data [ elmo:index "6"; rdfs:label "Visualisatie AP-NL"; html:link "query/dcat-nl-visualisatie"];
		];
		elmo:data [
			elmo:index "5";
			rdfs:label "KKG";
			elmo:data [ elmo:index "1"; rdfs:label "Lijst"; html:link "query/kkg-lijst"];
			elmo:data [ elmo:index "2"; rdfs:label "Vocabulaire"; html:link "query/kkg-model"];
			elmo:data [ elmo:index "3"; rdfs:label "Visualisatie"; html:link "query/kkg-visualisatie"];
		];
		elmo:data [
			elmo:index "9";
			rdfs:label "Sync";
			elmo:data [ elmo:index "1"; rdfs:label "Alles (initial start)"; html:link "update/sync"];
			elmo:data [ elmo:index "2"; rdfs:label "Requirements"; html:link "update/sync-requirements"];
			elmo:data [ elmo:index "3"; rdfs:label "Alle imports"; html:link "update/sync-imports"];
			elmo:data [ elmo:index "4"; rdfs:label "Model"; html:link "update/sync-model"];
			elmo:data [ elmo:index "5"; rdfs:label "Begrippen"; html:link "update/sync-concepts"];
		]
	];
