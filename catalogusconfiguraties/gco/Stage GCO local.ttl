	elmo:fragment [
		elmo:applies-to "uri";
		elmo:appearance elmo:GlobalLink;
		rdfs:label "URI";
	];
	elmo:fragment [
		elmo:applies-to "comment";
		rdfs:label "Toelichting";
	];
	elmo:query '''
		prefix sh: <http://www.w3.org/ns/shacl#>
