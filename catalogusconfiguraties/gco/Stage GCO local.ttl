stage:dcat-nl_overig a elmo:Part;
	elmo:index "4";
	elmo:appearance elmo:ShortTableAppearance;
	elmo:fragment [
		elmo:applies-to "shape";
		rdfs:label "Overige referentieklasse";
	];
	elmo:fragment [
		elmo:applies-to "uri";
		elmo:appearance elmo:GlobalLink;
