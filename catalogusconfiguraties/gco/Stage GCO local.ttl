	elmo:contains stage:inspire-lijst;
	elmo:contains stage:inspire-lijst2;
  elmo:query '''
    prefix doc: <http://localhost:8080/catalogus/gco/doc#>
    construct {
      doc:dcat-ap-eu elmo:data ?data.
      ?item rdf:first ?first.
      ?item rdf:rest ?rest.
      ?first ?p ?o
    }
    where {
      graph <@STAGE@> {
        doc:inspire elmo:data ?data.
        ?data rdf:rest* ?item.
        ?item rdf:first ?first.
        ?item rdf:rest ?rest.
        OPTIONAL {?first ?p ?o}
      }
    }
