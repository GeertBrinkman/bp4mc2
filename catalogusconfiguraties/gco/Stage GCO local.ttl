stage:dcat-nl_datatype a elmo:Part;
	elmo:index "5";
	elmo:appearance elmo:ShortTableAppearance;
	elmo:fragment [
		elmo:applies-to "shape";
		rdfs:label "Klassenaam";
	];
	elmo:fragment [
		elmo:applies-to "uri";
		elmo:appearance elmo:GlobalLink;
