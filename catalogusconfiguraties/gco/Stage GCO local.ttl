					?shape rdf:type sh:NodeShape.
					?shape rdfs:label ?shape_label.
					OPTIONAL {?shape sh:targetClass ?vocterm}
					BIND ("Node" as ?type)
				}
				UNION
				{
					?node sh:property ?shape.
					?node rdfs:label ?node_label.
					?shape rdfs:label ?shape_label.
					OPTIONAL {?shape sh:path ?vocterm}
					BIND ("Property" as ?type)
				}
			}
		}
	''';
