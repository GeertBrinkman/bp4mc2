@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix metaap: <http://bp4mc2.org/profiles/meta-ap-sc#>.

metaap:Datatype_string a sh:NodeShape;
	sh:targetClass xsd:string;
	sh:name "xsd:string";
	rdfs:label "Tekst"@nl;
	rdfs:label "Text"@en;
.

metaap:Datatype_dateTime a sh:NodeShape;
	sh:targetClass xsd:dateTime;
	sh:name "xsd:dateTime";
	rdfs:label "Datum/tijd"@nl;
	rdfs:label "Date/time"@en;
.
