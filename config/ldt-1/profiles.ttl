stage:profile-query a elmo:Query;
	elmo:query '''
		CONSTRUCT {
			?s ?p ?o
		}
		WHERE {
			GRAPH <@STAGE@> {
				<@REPRESENTATION@> rdfs:seeAlso ?shapesgraph
			}
			GRAPH ?shapesgraph {
				?s ?p ?o
			}
		}
	'''
.
