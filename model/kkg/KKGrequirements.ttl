	sh:datatype xsd:string;
	sh:minCount 0;
	sh:maxCount 1;
.
req:Objecttype14 a reqo:Requirement;
	sh:targetClass model:Objecttype;
	rdfs:label "Een Objecttype kan een toelichting bezitten";
