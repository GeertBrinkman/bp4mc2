	sh:severity sh:Violation;	
.

sbs:BikePropsShape rdf:type sh:NodeShape;
	sh:targetClass samplebase:BikeProps;
	sh:property sbs:idShape;
	sh:severity sh:Violation;
.

sbs:BikernameShape rdf:type sh:NodeShape;
	sh:targetClass samplebase:Bikername;
