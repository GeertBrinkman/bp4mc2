	sh:severity sh:Violation;
.

sbs:WeightShape rdf:type sh:NodeShape;
	sh:targetClass samplebase:Weight;
	sh:property sbs:amountShape;
	sh:property sbs:unitShape;
	sh:severity sh:Violation;
