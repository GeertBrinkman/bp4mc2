	sh:or ( 
		[sh:datatype xsd:CharacterString;]
		[sh:datatype xsd:Real;]
		[sh:datatype xsd:Date;]
		[sh:datatype xsd:DateTime;]
		[sh:datatype xsd:Year;]
		[sh:datatype xsd:Day;]
		[sh:datatype xsd:Month;]
		[sh:datatype xsd:Resource;]
		)
	]
