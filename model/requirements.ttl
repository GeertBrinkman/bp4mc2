#---------------------
# Controlled vocabularies
#---------------------

req:ControlledVocabulary rdf:type sh:NodeShape;
  rdfs:label "Asset (Waardelijst)"@nl;
  rdfs:label "Asset (ControlledVocabulary)"@en;
  sh:name "adms:Asset (ControlledVocabulary)";
  rdfs:comment "Een waardelijst is een verzameling van begrippen of elementen die worden gebruikt om inhoud te indexeren en / of inhoud op te halen via browsen of zoeken. Hij bevat meestal voorkeurs- en varianttermen en heeft een gedefinieerde scope of beschrijft een specifiek domein."@nl;
  rdfs:comment "A controlled vocabulary is an organized arrangement of concepts or elements used to index content and/or to retrieve content through browsing or searching. It typically includes preferred and variant terms and has a defined scope or describes a specific domain."@en;
  skos:example "De lijst met Kadastrale gemeenten."@nl;
  sh:property req:ControlledVocabularyLabel;
  sh:property req:ControlledVocabularyType;
#  sh:property req:ControlledVocabularyType;
  sh:property req:ControlledVocabularyPartOf;
  sh:property req:ControlledVocabularyDistribution;
  sh:property req:ControlledVocabularySpecification;
  sh:property req:ControlledVocabularyPartOf;
  sh:property req:ControlledVocabularyLandingPage
.
req:ControlledVocabularyLabel rdf:type sh:PropertyShape;
  rdfs:label "naam"@nl;
  rdfs:label "label"@en;
  sh:name "rdfs:label";
  sh:path rdfs:label;
  rdfs:comment "Een waardelijst heeft een naam."@nl;
  rdfs:comment "A controlled vocabulary has a label."@nl;
  skos:example "Landenlijst";
  sh:minCount 1;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:message "Een waardelijst heeft een naam."@nl;
  sh:message "A controlled vocabulary has a label."@en
.
req:ControlledVocabularyType a sh:PropertyShape;
  sh:name "rdf:type";
  sh:path rdf:type;
  sh:hasValue adms:Asset;
  rdfs:comment "Een waardelijst wordt getypeerd als adms:Asset."@nl;
  rdfs:comment "A controlled vocabulary is is typed as adms:Asset."@en;
  skos:example "De lijst met Kadastrale gemeenten is vindbaar als een adms:Asset."@nl;
.
#req:Waardelijst_typeWaardelijst a sh:PropertyShape;
#  rdfs:label "type asset = Waardelijst";
